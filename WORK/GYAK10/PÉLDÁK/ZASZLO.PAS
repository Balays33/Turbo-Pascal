  x1:=10;x2:=210;y1:=10;y2:=190;
  detectgraph(gd,gm);
  filemode:=0;
  initgraph(gd,gm,'e:\tp\bgi');
  setfillstyle(1,green);
  bar(x1,y1,x2,y2);
  setfillstyle(1,yellow);
  bar(10,70,210,130);
  setfillstyle(1,red);
  fillpoly(4,haromsz);
  setfillstyle(1,black);
  fillpoly(5,csillag);
  fillpoly(5,cs2);

readln;

end.

